<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets" inherit_id="point_of_sale.assets">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/custom_pos_keyboard_shortcut/static/src/js/screens.js"/>
        </xpath>
        <xpath expr="//link[@id='pos-stylesheet']" position="after">
            <link rel="stylesheet" href="/custom_pos_keyboard_shortcut/static/src/css/custom_pos.css"/>
        </xpath>
    </template>

</odoo>